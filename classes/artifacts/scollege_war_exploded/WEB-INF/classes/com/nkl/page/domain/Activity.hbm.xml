<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.nkl.page.domain.Activity" table="activity">
		<id name="activity_id" type="java.lang.Integer">
			<column name="activity_id" />
			<generator class="native" />
		</id>
		<property name="user_id" type="java.lang.Integer">
			<column name="user_id"  />
		</property>
		<property name="activity_title" type="java.lang.String">
			<column name="activity_title"  length="225" />
		</property>
		<property name="activity_content" type="java.lang.String">
			<column name="activity_content"  length="65535" />
		</property>
		<property name="activity_date" type="java.lang.String">
			<column name="activity_date"  length="50" />
		</property>
		<property name="activity_address" type="java.lang.String">
			<column name="activity_address"  length="300" />
		</property>
		<property name="activity_equip" type="java.lang.String">
			<column name="activity_equip"  length="300" />
		</property>
		<property name="activity_money" type="java.lang.Double">
			<column name="activity_money"  precision="22" scale="0" />
		</property>
		<property name="activity_type" type="java.lang.Integer">
			<column name="activity_type"  />
		</property>
		<property name="activity_flag" type="java.lang.Integer">
			<column name="activity_flag"  />
		</property>
	</class>
</hibernate-mapping>
