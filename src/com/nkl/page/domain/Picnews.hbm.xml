<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.nkl.page.domain.Picnews" table="picnews">
		<id name="picnews_id" type="java.lang.Integer">
			<column name="picnews_id" />
			<generator class="native" />
		</id>
		<property name="picnews_title" type="java.lang.String">
			<column name="picnews_title"  length="225" />
		</property>
		<property name="picnews_picture" type="java.lang.String">
			<column name="picnews_picture"  length="225" />
		</property>
		<property name="picnews_content" type="java.lang.String">
			<column name="picnews_content"  length="65535" />
		</property>
		<property name="picnews_admin" type="java.lang.String">
			<column name="picnews_admin"  length="50" />
		</property>
		<property name="picnews_date" type="java.lang.String">
			<column name="picnews_date"  length="50" />
		</property>
		<property name="picnews_number" type="java.lang.Integer">
			<column name="picnews_number"  />
		</property>
	</class>
</hibernate-mapping>
